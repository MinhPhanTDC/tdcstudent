{
  "name": "the-design-council",
  "private": true,
  "packageManager": "pnpm@9.15.0",
  "scripts": {
    "dev": "turbo dev",
    "build": "node scripts/validate-env.js && turbo build",
    "build:analyze": "turbo build && node scripts/build-analyze.js",
    "lint": "turbo lint",
    "typecheck": "turbo typecheck",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "clean": "turbo clean && rimraf node_modules",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "env:check": "node scripts/validate-env.js",
    "env:check:strict": "node scripts/validate-env.js --strict",
    "deploy": "bash scripts/deploy.sh",
    "deploy:auth": "bash scripts/deploy.sh auth",
    "deploy:admin": "bash scripts/deploy.sh admin",
    "deploy:student": "bash scripts/deploy.sh student",
    "deploy:rules": "bash scripts/deploy.sh rules",
    "deploy:dry-run": "bash scripts/deploy.sh --dry-run",
    "rules:validate": "node scripts/validate-rules.js"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.0",
    "@testing-library/react": "^14.2.0",
    "@testing-library/user-event": "^14.5.0",
    "@types/node": "^20.11.0",
    "@vitejs/plugin-react": "^4.2.0",
    "@vitest/coverage-v8": "^1.2.0",
    "fast-check": "^4.5.3",
    "jsdom": "^24.0.0",
    "prettier": "^3.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.5",
    "turbo": "^2.3.0",
    "typescript": "^5.3.0",
    "vitest": "^1.2.0"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  },
  "dependencies": {
    "undici": "^7.18.2"
  }
}
